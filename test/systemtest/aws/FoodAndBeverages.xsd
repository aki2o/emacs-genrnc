<?xml version="1.0"?>
<!-- Revision="$Revision: #4 $" -->
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
	<!--
  			 $Date: 2008/08/14/ $      
         AMAZON.COM CONFIDENTIAL.  This document and the information contained in it are
